component ICEPulseOx extends ICEDevice {
  override 'type = DeviceType '(IEEE ... Pulse Ox)
  
  inv 1..* pysioParams : ICESpO2
  inv 1..* pysioParams : ICEPulseRate
}

component ICESpO2 extends ICEPhysioParameter {
  override ppType = PhysioParameterType'(SpO2 Measurement) // from nomenclature
  override unit            = Unit'(Percent)                // from nomenclature
  
  // add exchanges
}

component ICEPulseRate extends ICEPhysioParameter {
  override ppType = PhysioParameterType'(PulseRate Measurement) // from nomenclature
  override unit        = Unit'(PulseRate Unit)                   // from nomenclature
}

component ICEBloodPressure extends ICEDevice {
  override 'type = DeviceType '(IEEE ... Blood Pressure)

  inv 1..* pysioParams : ICEBloodPressureParam
}

component ICEBloodPressureParam extends ICEPhysioParameter {
  override systolic    = PhysioParameterType'(BP Measurement S) // from nomenclature
  override diastolic   = PhysioParameterType'(BP Measurement D) // from nomenclature
  override unit        = Unit'(BP Unit)                   // from nomenclature
}

component ICEMultiMonitor extends ICEDevice {
  override 'type = DeviceType '(IEEE ... MultiMonitor)
  
  inv 1..* pysioParams : ICESpO2
  inv 1..* pysioParams : ICEPulseRate
  inv 1..* pysioParams : ICEBloodPressureParam
}
